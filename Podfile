# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'

target 'Restaurants' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Restaurants
  pod 'RxSwift'
  pod 'RxCocoa'
  pod 'Moya/RxSwift'
  pod 'NSObject+Rx'
  pod 'RxSwiftExt'
  pod 'RxMKMapView'
  pod 'RxOptional'
  pod 'RxGesture'
  pod 'SDWebImage'
  
  
  target 'RestaurantsTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'RestaurantsUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
